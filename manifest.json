{
    "name": "axolotl.nanuc",
    "version": "2.0.2",
    "description": "A Signal compatible messaging client for Ubuntu phones",
    "title": "Axolotl",
    "architecture": "@CLICK_ARCH@",
    "framework" : "ubuntu-sdk-20.04",
    "hooks": {
        "axolotl": {
            "apparmor": "axolotl.apparmor",
            "desktop": "axolotl.desktop",
            "urls": "axolotl.url-dispatcher",
            "content-hub": "axolotl.content-hub"
        },
        "push": {
            "apparmor": "axolotl-push.apparmor",
            "push-helper": "axolotl-push-helper.json"
        }
    },
    "icon": "qml/phoneui/images/logo.png",
    "maintainer": "Aaron Kimmig <aaron@nanu-c.org>"
}
